<script type="text/javascript">
    require(['ororeminder/js/subscriber', 'orosync/js/sync/wamp'], function(subscriber, Wamp){
        subscriber.init({{ app.user.id }}, new Wamp({
            host: '{{ ws.host == '*' ? app.request.headers.get('host') : ws.host }}',
            port: {{ ws.port }}
        }), '{{ getNotShownRemindersJson()|raw }}');
    });
</script>
