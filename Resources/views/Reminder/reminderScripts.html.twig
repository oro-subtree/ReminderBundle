{% if app.user %}
    {% placeholder orocrm_reminder_reminder_templates %}
    <script type="text/javascript">
        require(['ororeminder/js/subscriber', 'oroui/js/mediator'], function(subscriber, mediator){
            mediator.once('page-rendered', function () {
                {% if check_ws() %}
                    subscriber.init({{ app.user.id }}, {{ oro_reminder_get_requested_reminders_data()|json_encode|raw }});
                {% else %}
                    subscriber.showReminders({{ oro_reminder_get_requested_reminders_data()|json_encode|raw }});
                {% endif %}
                mediator.on('hash_navigation_request:complete', function(){
                    subscriber.reloadReminders();
                });
            });
        });
    </script>
{% endif %}
